{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".plasmo/index.d.ts",
    "./src/*.ts",
    "./src/*.tsx"
  ],
  "compilerOptions": {
    "paths": {
      "~*": [
        "./*"
      ],
      "@mui/styled-engine": [
        "./node_modules/@mui/styled-engine-sc"
      ]
    },
    "baseUrl": "."
  }
}
