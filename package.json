{
  "name": "comprueba-bulo",
  "displayName": "Verificar texto",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "manu1",
  "packageManager": "npm@9.1.2",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.15",
    "i18next": "^22.4.13",
    "plasmo": "^0.75.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.2.0",
    "react-router-dom": "^6.10.0",
    "styled-components": "^5.3.9",
    "styled-theming": "^2.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "^3.6.3",
    "@types/chrome": "^0.0.227",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-plugin-react": "^7.31.11",
    "prettier": "^2.8.7",
    "typescript": "^5.0.3"
  },
  "manifest": {
    "host_permissions": [
      "<all_urls>"
    ],
    "permissions": [
      "contextMenus",
      "tabs",
      "activeTab",
      "scripting",
      "storage",
      "windows"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "icons/*.png",
          "local-json-responses/*.json"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": []
      }
    ]
  }
}
